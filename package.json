{
  "name": "@ikenxuan/amagi",
  "version": "3.2.4",
  "description": "抖音、B站的 web 端相关数据接口基于 Node.js 的实现",
  "homepage": "https://github.com/ikenxuan/amagi",
  "bugs": {
    "url": "https://github.com/ikenxuan/amagi/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ikenxuan/amagi.git"
  },
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json && npm run fix",
    "dev": "tsx watch src/dev.ts",
    "dev:js": "node src/dev.js",
    "fix": "npx eslint lib/**/*.js --fix && npx eslint lib/**/*.d.ts --fix",
    "sort": "npx sort-package-json",
    "test": "node lib/test.js"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "chalk": "^5.3.0",
    "fastify": "^4.28.1",
    "lodash": "^4.17.21",
    "log4js": "6.9.0",
    "pino-pretty": "^12.1.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.8.6",
    "@typescript-eslint/parser": "^8.12.2",
    "eslint": "^9.14.0",
    "globals": "^15.11.0",
    "neostandard": "^0.11.7",
    "sort-package-json": "^2.10.1",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}