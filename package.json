{
  "name": "node-amagi",
  "version": "1.3.7",
  "type": "module",
  "main": "lib/index.js",
  "homepage": "https://github.com/ikenxuan/amagi",
  "scripts": {
    "app": "node lib/index.js",
    "start": "pm2 start lib/pm2.json",
    "stop": "pm2 stop lib/pm2.json",
    "log": "pm2 log amagi",
    "restart": "pm2 restart lib/pm2.json",
    "delpkg": "node src/pkg.js --type del",
    "dev": "tsx watch src/index.ts",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json && tsx src/build.ts && tsx src/cp.ts && npm run delpkg"
  },
  "files": [
    "/lib/**/*.js",
    "/lib/**/*.d.ts",
    "/config/*.yaml",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "bugs": {
    "url": "https://github.com/ikenxuan/amagi/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ikenxuan/amagi.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "chokidar": "^3.6.0",
    "lodash": "4.17.21",
    "fastify": "^4.28.1",
    "log4js": "^6.9.1",
    "md5": "^2.3.0",
    "node-fetch": "^3.3.2",
    "pm2": "^5.4.2",
    "yaml": "^2.4.5"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.7",
    "@types/md5": "latest",
    "@types/node": "latest",
    "eslint": "^9.7.0",
    "ts-node": "latest",
    "tsc-alias": "latest",
    "tsx": "latest",
    "typescript": "latest"
  }
}