{
  "name": "@ikenxuan/amagi",
  "version": "5.12.2",
  "description": "抖音、B站的 web 端相关数据接口基于 Node.js 的实现",
  "keywords": [
    "douyin",
    "bilibili",
    "kuaishou",
    "api"
  ],
  "homepage": "https://github.com/ikenxuan/amagi",
  "bugs": {
    "url": "https://github.com/ikenxuan/amagi/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ikenxuan/amagi.git"
  },
  "license": "GPL-3.0-only",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/default/index.d.ts",
      "development": "./src/index.ts",
      "node": {
        "import": "./dist/default/index.js",
        "require": "./dist/default/index.cjs"
      },
      "module": "./dist/default/index.js",
      "import": "./dist/default/index.js",
      "require": "./dist/default/index.cjs"
    },
    "./express": {
      "types": "./dist/exports/express.d.ts",
      "import": "./dist/exports/express.js",
      "require": "./dist/exports/express.cjs"
    },
    "./axios": {
      "types": "./dist/exports/axios.d.ts",
      "import": "./dist/exports/axios.js",
      "require": "./dist/exports/axios.cjs"
    },
    "./log4js": {
      "types": "./dist/exports/log4js.d.ts",
      "import": "./dist/exports/log4js.js",
      "require": "./dist/exports/log4js.cjs"
    },
    "./chalk": {
      "types": "./dist/exports/chalk.d.ts",
      "import": "./dist/exports/chalk.js",
      "require": "./dist/exports/chalk.cjs"
    }
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "tsdown && node ci/module.js",
    "clean": "npm ci/pr.js clean",
    "dev": "cross-env LOG_LEVEL=debug tsx watch src/dev.ts",
    "dev:js": "node src/dev.js",
    "docs:build": "typedoc --lang zh",
    "docs:serve": "typedoc --lang zh --serve",
    "fix": "npx eslint src/**/*.ts --fix",
    "pr": "node ci/pr.js",
    "pub": "npm publish --access public",
    "pub-beta": "npm publish --tag beta",
    "sort": "npx sort-package-json"
  },
  "dependencies": {
    "@ikenxuan/xhshow-ts": "^1.0.1",
    "axios": "^1.13.2",
    "chalk": "5.6.2",
    "express": "^5.2.1",
    "log4js": "npm:@karinjs/log4js@1.5.4",
    "zod": "4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "9.37.0",
    "@swc/core": "1.13.20",
    "@types/express": "5.0.6",
    "@types/node": "24.10.1",
    "@typescript-eslint/eslint-plugin": "8.46.0",
    "@typescript-eslint/parser": "8.46.0",
    "cross-env": "10.1.0",
    "eslint": "9.39.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "16.5.0",
    "neostandard": "0.12.2",
    "sort-package-json": "3.5.0",
    "tsc-alias": "1.8.16",
    "tsdown": "^0.17.0",
    "tsx": "4.21.0",
    "typedoc": "0.28.15",
    "typescript": "5.8.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}